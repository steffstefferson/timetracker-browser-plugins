{
  "manifest_version": 2,
  "name": "Timekeeper Jira",
  "description": "Timekeeper Jira",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "version": "1.1.0.0",
  "author": "Stef KÃ¤ser",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
		"firebase-4.1.1.js",
        "contentscript.js"
      ]
    }
  ]
}